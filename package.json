{
  "name": "koa-es6",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "rm -rf dist && npm run babel && node dist/bin/www.js",
    "dev": "npm run babel-watch & babel-node ./node_modules/.bin/nodemon dist/bin/www.js",
    "prd": "pm2 start dist/bin/www.js --name 'service' -- run start",
    "babel": "babel src --out-dir dist",
    "babel-watch": "babel src --watch --out-dir dist",
    "aa": "gulp"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "core-js": "^3.6.5",
    "nodemon": "^1.19.1"
  }
}
